    <externalResourceBindings>
      <externalResourceBinding>
        <key>BARTtmpdirectory</key>
        <resourceName>BARTtmpdirectory</resourceName>
      </externalResourceBinding>
         <externalResourceBinding>
        <key>BARTconfigdirectory</key>
        <resourceName>BARTconfigdirectory</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  
</resourceManagerConfiguration>

