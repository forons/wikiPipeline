<?xml version="1.0" encoding="UTF-8" ?>
<resourceManagerConfiguration xmlns="http://uima.apache.org/resourceSpecifier">
<name>BartTMPdirs</name>
<description></description>
<version>1.0</version>

    <externalResources>
      <externalResource>
        <name>BARTtmpdirectory</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>file:resources/limosine/tmp/bartdatadir</fileUrl>
        </fileResourceSpecifier>
        <implementationName>it.unitn.limosine.util.SharedModel_Impl</implementationName>
      </externalResource>
      <externalResource>
        <name>BARTconfigdirectory</name>
        <description/>
